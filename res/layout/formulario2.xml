<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" 
    android:background = "#E0F8F1" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="12dp"
        android:layout_y="14dp"
        android:text="Muchas gracias" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="12dp"
        android:layout_y="47dp"
        android:text="Ud a enviado un/una" />

    <TextView
        android:id="@+id/tvSpinner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="182dp"
        android:layout_y="47dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/tvComentario"
        android:layout_width="258dp"
        android:layout_height="76dp"
        android:layout_x="20dp"
        android:layout_y="97dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/tvNombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="132dp"
        android:layout_y="16dp"
        android:text="TextView" />

    <Button
        android:id="@+id/btnVol"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="126dp"
        android:layout_y="369dp"
        android:onClick="volver"
        android:text="Volver" />

    <TextView
        android:id="@+id/tvApellido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="201dp"
        android:layout_y="17dp"
        android:text="TextView" />

</AbsoluteLayout>